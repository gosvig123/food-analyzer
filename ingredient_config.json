      "tomato",
      "potato",
      "onion",
      "chicken",
      "beef",
      "pork",
      "salmon",
      "tuna"
    ]
  },
  "cleaning_patterns": [
    "\\b(raw|cooked|fresh|frozen|canned|dried|organic)\\b",
    "\\b(whole|half|quarter|sliced|diced|chopped)\\b",
    "\\b(large|small|medium|mini)\\b",
    "\\([^)]*\\)",
    "\\d+\\s*(oz|lb|kg|g|ml|l)\\b"
  ],
  "generic_terms": [
    "food",
    "item",
    "product",
    "dish",
    "meal",
    "snack",
    "beverage"
  ],
  "cache_maxsize": 1,
  "cache_file": "ingredient_cache.json",
