{
  "apis": {
    "usda": {
      "base_url": "https://api.nal.usda.gov/fdc/v1/foods/search",
      "timeout": 10,
      "page_size": 50,
      "max_results": 500,
      "headers": {
        "Accept": "application/json"
      },
      "params": {
        "dataType": ["Foundation", "SR Legacy"],
        "sortBy": "publishedDate",
        "sortOrder": "desc"
      }
    },
    "openfoodfacts": {
      "base_url": "https://world.openfoodfacts.org/cgi/ingredients.pl",
      "timeout": 15,
      "page_size": 100,
      "max_results": 500,
      "headers": {
        "User-Agent": "FoodAnalyzer/1.0"
      },
      "params": {
        "json": "1"
      }
    }
  },
  "models": {
    "clip": {
      "name": "ViT-B-32",
      "pretrained": "laion2b_s34b_b79k",
      "embedding_threshold": 0.8,
      "max_results": 200
    },
    "efficientnet": {
      "name": "efficientnet_b0",
      "pretrained": null,
      "embedding_threshold": 0.8,
      "max_results": 300
    }
  },
  "search_categories": [
    "fruits",
    "vegetables",
    "grains",
    "dairy",
    "meat",
    "seafood",
    "nuts",
    "spices",
    "herbs",
    "beans",
    "oils",
    "proteins"
  ],
  "food_keywords": {
    "categories": [
      "fruits",
      "vegetables",
      "meat",
